syntax enable
set number
set tabstop=4
set softtabstop=4
set expandtab
set number
set showcmd
set cursorline
filetype plugin on
filetype indent on
set wildmenu
set lazyredraw
set showmatch
set incsearch
set hlsearch

set laststatus=2
set noshowmode

nnoremap Ctrl+C Esc

map <F2> :NERDTreeToggle<CR>
